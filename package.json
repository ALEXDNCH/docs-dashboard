{
  "name": "doc_parser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3010",
    "build": "vue-tsc -b && npm run lint && vite build",
    "preview": "vite preview --port 3020",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .ts,.vue src",
    "dev:reset": "docker compose -f docker-compose.dev.yml down -v --remove-orphans && docker builder prune -f && docker compose -f docker-compose.dev.yml up --build",
    "dev:docker": "docker compose -f docker-compose.dev.yml up --build",
    "prod:docker:build": "docker build -t doc_parser .",
    "prod:docker:run": "docker run --rm -p 8080:80 doc_parser",
    "dev:docker:stop": "docker compose -f docker-compose.dev.yml down --remove-orphans",
    "prod:docker:stop": "docker stop doc_parser_prod && docker rm doc_parser_prod"
  },
  "dependencies": {
    "@fontsource/montserrat": "^5.2.6",
    "@tanstack/vue-query": "^5.85.5",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.6.2",
    "sass": "^1.91.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.0.5"
  }
}
